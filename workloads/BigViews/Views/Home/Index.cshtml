@using BigViews
@model IEnumerable<User>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Simple Template Renderer sample.</title>
    <style>
        .contact-card {
            border: 1px solid #000000;
        }
    </style>
</head>
<body>
    @foreach (var contact in Model)
    {
        await Html.RenderPartialAsync("_User", contact);
    }

    This is a simple View.
</body>
</html>